# =====================================================
# Freelance LLC OS - Environment Configuration
# =====================================================
# Copy this file to .env and fill in your actual values
# DO NOT commit the .env file to version control
# =====================================================

# LLC Information
# ðŸ”´ YOUR ACTION NEEDED: Fill in your LLC details
LLC_NAME="Your LLC Name Here"
OWNER_NAME="Your Full Name"
OWNER_EMAIL="you@yourllc.com"
LLC_EIN="12-3456789"

# Billing Configuration
# ðŸ”´ YOUR ACTION NEEDED: Set your default hourly rate
DEFAULT_HOURLY_RATE=150

# Stripe Integration
# ðŸ”´ YOUR ACTION NEEDED: Get your Stripe API keys from https://dashboard.stripe.com/apikeys
STRIPE_SECRET_KEY="sk_test_your_secret_key_here"
STRIPE_PUBLISHABLE_KEY="pk_test_your_publishable_key_here"
STRIPE_WEBHOOK_SECRET="whsec_your_webhook_secret_here"

# AI Model Configuration
# ðŸ”´ YOUR ACTION NEEDED: Choose your preferred Claude model
# Options: claude-3-5-sonnet-20241022, claude-3-opus-20240229, claude-3-sonnet-20240229
CLAUDE_MODEL="claude-3-5-sonnet-20241022"

# Vault Path (where your business data is stored)
# ðŸ”´ YOUR ACTION NEEDED: Set the absolute path to your vault directory
VAULT_PATH="/path/to/your/Freelance_Dev/vault"

# Tax Configuration
# ðŸ”´ YOUR ACTION NEEDED: Set your estimated quarterly tax rate (as decimal, e.g., 0.25 for 25%)
QUARTERLY_TAX_RATE=0.25

# Email Configuration (optional - for invoice sending)
# ðŸ”´ YOUR ACTION NEEDED: Fill in if you want automated email invoicing
SMTP_HOST="smtp.gmail.com"
SMTP_PORT=587
SMTP_USER="your-email@gmail.com"
SMTP_PASSWORD="your-app-password"

# Business Development
# ðŸ”´ YOUR ACTION NEEDED: Define your strategic pillars (comma-separated)
STRATEGIC_PILLARS="Cloud Architecture,DevOps,AI/ML Integration"

# Notification Thresholds
RENEWAL_ALERT_DAYS=30
LOW_CLIENT_HEALTH_THRESHOLD=5
MAX_P0_TASKS=3
